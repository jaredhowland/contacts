{
  "name": "jaredhowland/contacts",
  "description": "PHP library to create files to store contact information",
  "keywords": [
    "contacts",
    "address book",
    "vcard"
  ],
  "homepage": "http://github.com/jaredhowland/contacts",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Contacts\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Jared Howland",
      "homepage": "http://jaredhowland.com",
      "role": "developer"
    }
  ],
  "require": {
    "php": "^8.0",
    "guzzlehttp/guzzle": "^7.0",
    "ext-fileinfo": "*",
    "ext-exif": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10"
  }
}
